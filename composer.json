{
  "name":"okvpn/datadog-symfony",
  "type":"symfony-bundle",
  "license":"MIT",
  "description": "Symfony Datadog integration",
  "keywords": ["DogStatsD", "datadog-symfony", "php-datadog"],
  "authors": [
    {
      "name": "Uladzimir Tsykun",
      "homepage": "https://github.com/vtsykun",
      "email": "vtsykun@okvpn.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Okvpn\\Bundle\\DatadogBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": { "Okvpn\\Bundle\\DatadogBundle\\Tests\\": "tests/" }
  },
  "require": {
    "php":">=7.1",
    "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0",
    "symfony/yaml": "^2.8 || ^3.0 || ^4.0",
    "graze/dog-statsd": "^0.4"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "doctrine/doctrine-bundle": "^1.6.10",
    "doctrine/orm": "2.5.11 || 2.6.x-dev as 2.6.3",
    "phpunit/phpunit": "^6.2",
    "symfony/browser-kit": "^2.8 || ^3.4 || ^4.0",
    "symfony/console": "^2.8 || ^3.4 || ^4.0",
    "symfony/security-bundle": "^2.8 || ^3.4 || ^4.0",
    "symfony/phpunit-bridge": "^2.8 || ^3.4 || ^4.0",
    "symfony/var-dumper": "^2.8 || ^3.4 || ^4.0",
    "symfony/monolog-bundle": "^3.2.0"
  }
}
